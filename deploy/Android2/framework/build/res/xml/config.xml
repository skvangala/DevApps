<widget id="io.cordova.helloCordova" version="2.0.0" xmlns="http://www.w3.org/ns/widgets">
    <access origin="http://127.0.0.1*" />
    <access origin="*://*.googleapis.com/*" />
    <access origin="*://*.gstatic.com/*" />
    <access origin="*://elocation.oracle.com/*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="useBrowserHistory" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="loadInWebView" value="true" />
    <feature name="ADFMobileDeviceProperties">
        <param name="android-package" value="oracle.adfmf.phonegap.ADFMobileDeviceProperties" />
    </feature>
    <feature name="AdfmfSlidingWindowPlugIn">
        <param name="android-package" value="oracle.adfmf.phonegap.AdfmfSlidingWindowPlugIn" />
    </feature>
    <feature name="ADFMobileShell">
        <param name="android-package" value="oracle.adfmf.phonegap.ADFMobileShell" />
    </feature>
    <feature name="ADFMobileFeatures">
        <param name="android-package" value="oracle.adfmf.phonegap.ADFMobileFeatures" />
    </feature>
    <feature name="MafLocalNotification">
        <param name="android-package" value="oracle.adfmf.framework.localnotification.MafNativeLocalNotificationPlugin" />
    </feature>
</widget>
